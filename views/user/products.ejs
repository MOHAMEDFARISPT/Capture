<!DOCTYPE html>
<html lang="en">
    <%- include('../userpartials/head')%>
    <%- include('../userpartials/naavhead')%>



<!-- molla/product.html  22 Nov 2019 09:54:50 GMT -->


<body>
    

        <main class="main">
            <nav aria-label="breadcrumb" class="breadcrumb-nav border-0 mb-0">
                <div class="container d-flex align-items-center">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/">Home</a></li>
                        <li class="breadcrumb-item"><a href="#">Products</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Default</li>
                    </ol>

                  
                </div><!-- End .container -->
            </nav><!-- End .breadcrumb-nav -->


           
            <div class="page-content">
                <div class="container">
                    <div class="product-details-top">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="product-gallery product-gallery-vertical">
                                    
                                    <div class="row">
                                        <figure class="product-main-image">

                                            <img id="product-zoom" src="<%= product.productImage[0] %>" data-zoom-image="<%= product.productImage[0] %>" alt="product image">

                                            <a href="#" id="btn-product-gallery" class="btn-product-gallery">
                                                <i class="icon-arrows"></i>
                                            </a>
                                        </figure><!-- End .product-main-image -->

                                        <div id="product-zoom-gallery" class="product-image-gallery">
                                            <a class="product-gallery-item active" href="#" data-image="<%= product.productImage[0] %>" data-zoom-image="<%= product.productImage[0] %>">
                                                <img src="<%= product.productImage[0] %>" alt="product side">
                                            </a>

                                            <a class="product-gallery-item" href="#" data-image="<%= product.productImage[1] %>" data-zoom-image="<%= product.productImage[1] %>">
                                                <img src="<%= product.productImage[1] %>" alt="product cross">
                                            </a>

                                            <a class="product-gallery-item" href="#" data-image="<%= product.productImage[2] %>" data-zoom-image="<%= product.productImage[2] %>">
                                                <img src="<%= product.productImage[2] %>" alt="product with model">
                                            </a>

                                            <a class="product-gallery-item" href="#" data-image="<%= product.productImage[3] %>" data-zoom-image="<%= product.productImage[3] %>">
                                                <img src="<%= product.productImage[3] %>" alt="product back">
                                            </a>
                                        </div><!-- End .product-image-gallery -->
                                    </div><!-- End .row -->
                                </div><!-- End .product-gallery -->
                            </div><!-- End .col-md-6 -->

                            <div class="col-md-6">
                                <div class="product-details">
                                    <h1 class="product-title"><%= product.productname %></h1><!-- End .product-title -->
                                    <h1 class="product-title"><%= product.description %></h1><!-- End .product-title -->


                                   

                                    <div class="product-price">
                                        ₹ <%= product.price %>
                                    
                                    </div><!-- End .product-details-action -->

                                    <div class="product-details-footer">
                                       

                                       
                                    </div><!-- End .product-details-footer -->
                                </div><!-- End .product-details -->
                            </div><!-- End .col-md-6 -->
                        </div><!-- End .row -->
                    </div><!-- End .product-details-top -->
                </div>
                    
        
   <!-- End .page-wrapper -->
    <button id="scroll-top" title="Back to Top"><i class="icon-arrow-up"></i></button>

   
    <!-- Mobile Menu -->
    <div class="mobile-menu-overlay"></div><!-- End .mobil-menu-overlay -->

    </div>
    <footer class="footer">
        	

        <div class="footer-bottom">
            <div class="container">
                <p class="footer-copyright">Copyright © 2023 Sneakpeakhub Store. All Rights Reserved.</p><!-- End .footer-copyright -->
                <figure class="footer-payments">
                    <img src="assets/images/payments.png" alt="Payment methods" width="272" height="20">
                </figure><!-- End .footer-payments -->
            </div><!-- End .container -->
        </div><!-- End .footer-bottom -->
    </footer><!-- End .footer -->
    
    </main>

   

    <!-- Plugins JS File -->
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/bootstrap.bundle.min.js"></script>
    <script src="assets/js/jquery.hoverIntent.min.js"></script>
    <script src="assets/js/jquery.waypoints.min.js"></script>
    <script src="assets/js/superfish.min.js"></script>
    <script src="assets/js/owl.carousel.min.js"></script>
    <script src="assets/js/bootstrap-input-spinner.js"></script>
    <script src="assets/js/jquery.elevateZoom.min.js"></script>
    <script src="assets/js/bootstrap-input-spinner.js"></script>
    <script src="assets/js/jquery.magnific-popup.min.js"></script>
    <!-- Main JS File -->
    <script src="assets/js/main.js"></script>
</body>


<!-- molla/product.html  22 Nov 2019 09:55:05 GMT -->
</html>