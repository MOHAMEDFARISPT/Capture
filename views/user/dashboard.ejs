<h2>My Orders</h2>

<table>
    <thead>
        <tr>
            <th>Product Image</th>
            <th>Product Name</th>
            <th>Order ID</th>
            <th>Total</th>
            <th>Date</th>
            <th>Status</th>
            <th>Action</th>
            <th>Cancel</th> <!-- New column for Cancel button -->
        </tr>
    </thead>
    <!-- Assuming you have included the necessary HTML structure -->
    <tbody>
        <% if (orders && orders.length > 0) { %>
        <% orders.forEach(order => { %>
          <% order.products.forEach(product => { %>
            <tr>
              <td><img src="<%= product.productId.productImage[0] %>" alt="Product Image" width="50" height="50"></td>
              <td><%= product.productId.productname %></td>
              <td><%= order.orderId %></td>
              <td>$<%= product.total.toFixed(2) %></td>
              <td><%= formatDate(order.date) %></td>
              <td><%= product.cancelstatus %></td>
              <td>
                <a href="/orderdetails/<%= order._id %>?productid=<%= product.productId._id %>" class="btn btn-link">View Details</a>
              </td>
              <td>
               
                <!-- Inside your EJS template -->
     <button class="btn btn-danger" onclick="cancelOrder('<%= order._id %>', '<%= product.productId._id %>')">Cancel Order</button>

              
                
              </td>
            </tr>
          <% }); %>
        <% }); %>
        <% } else { %> 
            <tr>
                <td colspan="8">No Orders Available </td>
            </tr>
            <% } %>
          
    </tbody>
</table>



<!-- Other HTML content -->

<% function formatDate(date) {
    const options = { year: 'numeric', month: 'short', day: 'numeric' };
    return new Date(date).toLocaleDateString(undefined, options);
} %>
